{
  "extends": [
    "airbnb"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "react-hooks"],
  "parserOptions": {
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  },
  "rules": {
    "react/no-danger": 0,
    "react/jsx-one-expression-per-line": 0,
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0,
    "react/button-has-type": 0,
    "react/jsx-props-no-spreading": 0,
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": 0,
    "react/destructuring-assignment": 0,
    "jsx-a11y/anchor-has-content": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "jsx-a11y/click-events-have-key-events": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "jsx-a11y/control-has-associated-label": 0,
    "arrow-body-style": 0,
    "object-curly-newline": 0,
    "arrow-parens": 0,
    "max-len": 0,
    "no-underscore-dangle": 0,
    "quotes": 0,
    "camelcase": 0,
    "@typescript-eslint/camelcase": 0,
    "@typescript-eslint/explicit-function-return-type": 0,
    "@typescript-eslint/no-explicit-any": 0,
    "@typescript-eslint/no-empty-interface": 0,
    "no-unused-vars": [
      "error",
      {
        "varsIgnorePattern": "DocumentContext|NextPage|NextPageContext|ExchangeRate|Currency|Trip|ModalObject|Logger|WhiteLabelData|FormalFieldProps|FormalWebFieldProps|TCountryMap|TInvertCountryMap|TInvertedCountryMap|TActivity|TCity|IToast|LoggingEvents|Popup"
      }
    ],
    "no-console": 0,
    "no-extra-boolean-cast": 0,
    "import/extensions": 0,
    "react/require-default-props": 0
  },
  "globals": {
    "document": true,
    "fetch": true,
    "window": true,
    "navigator": true,
    "describe": true,
    "it": true,
    "expect": true,
    "Image": true,
    "jest": true,
    "self": true,
    "IntersectionObserver": true
  }
}